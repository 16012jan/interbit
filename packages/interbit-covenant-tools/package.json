{
  "name": "interbit-covenant-tools",
  "version": "0.4.53",
  "description": "Interbit platform covenants and covenant components",
  "main": "dist/interbit-covenant-tools.js",
  "scripts": {
    "prepack": "npm run build",
    "build": "webpack --env dev && webpack --env build",
    "build:watch": "webpack --env dev && webpack --env build --watch",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "jest"
  },
  "files": [
    "dist",
    "src/rootCovenant"
  ],
  "author": "BTL GROUP LTD",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-runtime": "7.1.0",
    "@babel/preset-env": "7.1.0",
    "assert": "^1.4.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-loader": "8.0.4",
    "jest": "23.6.0",
    "should": "^13.2.1",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "interbit-covenant-utils": "0.14.0",
    "json-stable-stringify": "^1.0.1",
    "object-hash": "^1.3.0",
    "seamless-immutable": "^7.1.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
