{
  "name": "interbit-ui-tools",
  "version": "0.4.53",
  "description": "UI tools including Redux middleware that boots an Interbit hypervisor and syncs the blockchain in your Redux state.",
  "main": "dist/interbit-ui-tools.js",
  "license": "MIT",
  "scripts": {
    "prepack": "npm run build",
    "build": "webpack --env dev && webpack --env build",
    "build:watch": "webpack --env dev && webpack --env build --watch",
    "dev": "webpack --progress --colors --watch --env dev",
    "test": "jest"
  },
  "keywords": [
    "Interbit",
    "blockchain",
    "redux",
    "middleware"
  ],
  "author": "BTL GROUP LTD",
  "devDependencies": {
    "@babel/cli": "7.1.2",
    "@babel/core": "7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "7.0.0",
    "@babel/plugin-transform-async-to-generator": "7.1.0",
    "assert": "^1.4.1",
    "babel-loader": "8.0.4",
    "fs-extra": "^5.0.0",
    "interbit-core": "0.14.0",
    "jest": "23.6.0",
    "redux-saga-test-plan": "^3.6.0",
    "webpack": "^3.10.0",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "base64-url": "^2.2.0",
    "localforage": "^1.7.2",
    "lodash": "^4.17.5",
    "query-string": "^5.0.1",
    "redux-saga": "^0.16.0",
    "seamless-immutable": "^7.1.2"
  },
  "jest": {
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
